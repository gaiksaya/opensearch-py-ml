lib = library(identifier: 'jenkins@support-assets-tar', retriever: modernSCM([
    $class: 'GitSCMSource',
    remote: 'https://github.com/gaiksaya/opensearch-build-libraries-1.git',
]))

standardReleasePipelineWithGenericTrigger(
    tokenIdCredential: 'jenkins-opensearch-py-ml-generic-webhook-token',
    causeString: 'A tag was cut on opensearch-project/opensearch-js repository causing this workflow to run',
    downloadReleaseAsset: true) {
        sh 'cd dist && ls -l'
        echo "Vola!"
    }